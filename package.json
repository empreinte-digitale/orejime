{
  "name": "orejime",
  "version": "2.3.1",
  "description": "An easy to use consent manager that focuses on accessibility. A fork of Klaro!.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empreinte-digitale/orejime.git"
  },
  "keywords": [
    "cmp",
    "consent management platform",
    "cookie",
    "consent",
    "gdpr",
    "compliance",
    "accessibility",
    "ecofriendly",
    "low footprint",
    "lightweight"
  ],
  "main": "lib/orejime.js",
  "browser": "lib/orejime.js",
  "module": "es/orejime.js",
  "scripts": {
    "start": "webpack watch",
    "serve": "webpack serve",
    "test": "jest src --passWithNoTests",
    "clean-build": "shx rm -Rf -Rf ./lib && shx rm -Rf -Rf ./es",
    "build-commonjs": "npx tsc -p ./build/tsconfig.commonjs.json",
    "build-module": "npx tsc -p ./build/tsconfig.module.json",
    "build-migrations": "webpack --config webpack.config.migrations.js",
    "build": "npm run clean-build && npm run build-commonjs && npm run build-module && webpack --mode production",
    "stats": "webpack --json > stats.json && npx webpack-bundle-analyzer stats.json -p 3001",
    "prepublishOnly": "npm run test && npm run build"
  },
  "author": "Emmanuel Pelletier <e.pelletier@empreintedigitale.fr>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/empreinte-digitale/orejime/issues"
  },
  "homepage": "https://github.com/empreinte-digitale/orejime#readme",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "clean-deep": "^3.4.0",
    "js-cookie": "^3.0.1",
    "react-modal": "^3.14.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@types/jest": "^27.5.0",
    "@types/js-cookie": "^3.0.2",
    "@types/react-modal": "^3.13.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "preact": "^10.23.2",
    "prettier": "^2.2.1",
    "prettier-loader": "^3.3.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "uneval.js": "^5.7.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "yaml-loader": "^0.8.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  }
}
