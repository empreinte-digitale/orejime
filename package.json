{
  "name": "orejime",
  "version": "2.3.1",
  "description": "An easy to use consent manager that focuses on accessibility. A fork of Klaro!.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empreinte-digitale/orejime.git"
  },
  "keywords": [
    "cmp",
    "consent management platform",
    "cookie",
    "consent",
    "gdpr",
    "compliance",
    "accessibility",
    "ecofriendly",
    "low footprint",
    "lightweight"
  ],
  "main": "lib/orejime.js",
  "browser": "lib/orejime.js",
  "module": "es/orejime.js",
  "scripts": {
    "start": "rspack --watch",
    "serve": "rspack serve",
    "test": "jest src --passWithNoTests",
    "clean-build": "shx rm -Rf ./lib ./es ./dist/*.js ./dist/*.css ./dist/*.scss ./dist/stats.json",
    "build-commonjs": "tsc -p ./build/tsconfig.commonjs.json",
    "build-module": "tsc -p ./build/tsconfig.module.json",
    "build-migrations": "rspack --config rspack.config.migrations.js",
    "build": "npm run clean-build && npm run build-commonjs && npm run build-module && rspack --mode production",
    "stats": "rspack build --analyze",
    "prepublishOnly": "npm run test && npm run build"
  },
  "author": "Emmanuel Pelletier <e.pelletier@empreintedigitale.fr>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/empreinte-digitale/orejime/issues"
  },
  "homepage": "https://github.com/empreinte-digitale/orejime#readme",
  "dependencies": {
    "clean-deep": "^3.4.0",
    "js-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@rspack/cli": "^1.0.3",
    "@rspack/core": "^1.0.3",
    "@types/jest": "^27.5.0",
    "@types/js-cookie": "^3.0.2",
    "@types/micromodal": "^0.3.5",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "micromodal": "^0.4.10",
    "preact": "^10.23.2",
    "prettier": "^2.2.1",
    "prettier-loader": "^3.3.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "uneval.js": "^5.7.2",
    "yaml-loader": "^0.8.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  }
}
